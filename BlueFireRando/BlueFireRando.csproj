<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <SupportedOSPlatformVersion>7.0</SupportedOSPlatformVersion>
    <DebugType>none</DebugType>
    <ProduceReferenceAssembly>False</ProduceReferenceAssembly>
    <Company>Blue Fire Modding</Company>
    <Authors>spuds</Authors>
    <Product>Blue Fire Randomiser</Product>
    <ApplicationIcon>beegUmby.ico</ApplicationIcon>
    <StartupObject>BlueFireRando.Startup</StartupObject>
    <BaseIntermediateOutputPath>obj\</BaseIntermediateOutputPath>
    <Description>An application that allows randomisation of items in Blue Fire</Description>
    <BaseOutputPath>\</BaseOutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <Optimize>False</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="beegUmby.ico" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Newtonsoft.Json">
      <HintPath>..\..\..\..\..\modding\BlueFireRando\UAssetAPI\UAssetAPI\bin\Debug\Newtonsoft.Json.dll</HintPath>
    </Reference>
    <Reference Include="UAssetAPI">
      <HintPath>..\..\..\..\..\modding\BlueFireRando\UAssetAPI\UAssetAPI\bin\Debug\UAssetAPI.dll</HintPath>
    </Reference>
  </ItemGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="rmdir /s /q &quot;C:\modding\BlueFireRando\BlueFireRando\obj&quot;&#xD;&#xA;del &quot;BlueFireRando.exe.config&quot;&#xD;&#xA;del &quot;UAssetAPI.pdb&quot;&#xD;&#xA;del &quot;UAssetAPI.xml&quot;" />
  </Target>

</Project>